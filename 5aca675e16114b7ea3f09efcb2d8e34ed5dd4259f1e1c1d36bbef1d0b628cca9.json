{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nimport \"core-js/modules/es.iterator.some.js\";\nimport { ref, computed } from 'vue';\nimport { useRouter } from 'vue-router';\nimport { useRestaurantsStore } from '../stores/restaurants';\nexport default {\n  name: 'HomeView',\n  setup() {\n    const router = useRouter();\n    const searchQuery = ref('');\n    const restaurantsStore = useRestaurantsStore();\n    const filteredRestaurants = computed(() => {\n      if (!searchQuery.value) return restaurantsStore.getAllRestaurants;\n      const query = searchQuery.value.toLowerCase();\n      return restaurantsStore.getAllRestaurants.filter(restaurant => restaurant.name.toLowerCase().includes(query) || restaurant.tags.some(tag => tag.toLowerCase().includes(query)));\n    });\n    const search = () => {\n      console.log('搜索:', searchQuery.value);\n    };\n    const navigateToRestaurant = id => {\n      router.push(`/restaurant/${id}`);\n    };\n    return {\n      searchQuery,\n      filteredRestaurants,\n      search,\n      navigateToRestaurant\n    };\n  }\n};","map":{"version":3,"names":["ref","computed","useRouter","useRestaurantsStore","name","setup","router","searchQuery","restaurantsStore","filteredRestaurants","value","getAllRestaurants","query","toLowerCase","filter","restaurant","includes","tags","some","tag","search","console","log","navigateToRestaurant","id","push"],"sources":["C:\\Users\\cll13\\Desktop\\01\\food-delivery-vuecli\\src\\views\\HomeView.vue"],"sourcesContent":["<template>\n  <div class=\"home\">\n    <header class=\"home-header\">\n      <h1>美食外卖</h1>\n      <div class=\"search-bar\">\n        <input type=\"text\" placeholder=\"搜索商家或美食\" v-model=\"searchQuery\" />\n        <button @click=\"search\">搜索</button>\n      </div>\n    </header>\n    \n    <main class=\"restaurants-container\">\n      <div class=\"restaurant-card\" v-for=\"restaurant in filteredRestaurants\" :key=\"restaurant.id\" @click=\"navigateToRestaurant(restaurant.id)\">\n        <!-- 使用public目录下的图片 -->\n        <img \n          :src=\"restaurant.image\" \n          :alt=\"restaurant.name\" \n          class=\"restaurant-image\" \n        />\n        <div class=\"restaurant-info\">\n          <h3>{{ restaurant.name }}</h3>\n          <div class=\"restaurant-meta\">\n            <span class=\"rating\">⭐ {{ restaurant.rating }}</span>\n            <span class=\"distance\">{{ restaurant.distance }}km</span>\n            <span class=\"delivery-time\">{{ restaurant.deliveryTime }}分钟</span>\n          </div>\n          <div class=\"restaurant-tags\">\n            <span v-for=\"tag in restaurant.tags\" :key=\"tag\" class=\"tag\">{{ tag }}</span>\n          </div>\n        </div>\n      </div>\n    </main>\n  </div>\n</template>\n\n<script>\nimport { ref, computed } from 'vue'\nimport { useRouter } from 'vue-router'\nimport { useRestaurantsStore } from '../stores/restaurants'\n\nexport default {\n  name: 'HomeView',\n  setup() {\n    const router = useRouter()\n    const searchQuery = ref('')\n    const restaurantsStore = useRestaurantsStore()\n\n    const filteredRestaurants = computed(() => {\n      if (!searchQuery.value) return restaurantsStore.getAllRestaurants\n      const query = searchQuery.value.toLowerCase()\n      return restaurantsStore.getAllRestaurants.filter(restaurant => \n        restaurant.name.toLowerCase().includes(query) ||\n        restaurant.tags.some(tag => tag.toLowerCase().includes(query))\n      )\n    })\n\n    const search = () => {\n      console.log('搜索:', searchQuery.value)\n    }\n\n    const navigateToRestaurant = (id) => {\n      router.push(`/restaurant/${id}`)\n    }\n\n    return {\n      searchQuery,\n      filteredRestaurants,\n      search,\n      navigateToRestaurant\n    }\n  }\n}\n</script>\n\n<style scoped>\n.home {\n  max-width: 1200px;\n  margin: 0 auto;\n  padding: 20px;\n}\n\n.home-header {\n  text-align: center;\n  margin-bottom: 30px;\n}\n\n.home-header h1 {\n  font-size: 2.5rem;\n  color: #e74c3c;\n  margin-bottom: 20px;\n}\n\n.search-bar {\n  display: flex;\n  max-width: 500px;\n  margin: 0 auto;\n}\n\n.search-bar input {\n  flex: 1;\n  padding: 12px;\n  font-size: 1rem;\n  border: 2px solid #ddd;\n  border-radius: 4px 0 0 4px;\n}\n\n.search-bar button {\n  padding: 12px 20px;\n  font-size: 1rem;\n  background-color: #e74c3c;\n  color: white;\n  border: none;\n  border-radius: 0 4px 4px 0;\n  cursor: pointer;\n}\n\n.restaurants-container {\n  display: grid;\n  grid-template-columns: repeat(auto-fill, minmax(300px, 1fr));\n  gap: 20px;\n}\n\n.restaurant-card {\n  border: 1px solid #ddd;\n  border-radius: 8px;\n  overflow: hidden;\n  cursor: pointer;\n  transition: transform 0.2s;\n}\n\n.restaurant-card:hover {\n  transform: translateY(-5px);\n  box-shadow: 0 5px 15px rgba(0, 0, 0, 0.1);\n}\n\n.restaurant-image {\n  width: 100%;\n  height: 200px;\n  object-fit: cover;\n}\n\n.restaurant-info {\n  padding: 15px;\n}\n\n.restaurant-info h3 {\n  margin: 0 0 10px 0;\n  color: #333;\n}\n\n.restaurant-meta {\n  display: flex;\n  gap: 15px;\n  margin-bottom: 10px;\n  font-size: 0.9rem;\n  color: #666;\n}\n\n.restaurant-tags {\n  display: flex;\n  gap: 8px;\n  flex-wrap: wrap;\n}\n\n.tag {\n  background-color: #f0f0f0;\n  padding: 4px 8px;\n  border-radius: 4px;\n  font-size: 0.8rem;\n  color: #666;\n}\n</style>"],"mappings":";;;;AAmCA,SAASA,GAAG,EAAEC,QAAO,QAAS,KAAI;AAClC,SAASC,SAAQ,QAAS,YAAW;AACrC,SAASC,mBAAkB,QAAS,uBAAsB;AAE1D,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,KAAKA,CAAA,EAAG;IACN,MAAMC,MAAK,GAAIJ,SAAS,CAAC;IACzB,MAAMK,WAAU,GAAIP,GAAG,CAAC,EAAE;IAC1B,MAAMQ,gBAAe,GAAIL,mBAAmB,CAAC;IAE7C,MAAMM,mBAAkB,GAAIR,QAAQ,CAAC,MAAM;MACzC,IAAI,CAACM,WAAW,CAACG,KAAK,EAAE,OAAOF,gBAAgB,CAACG,iBAAgB;MAChE,MAAMC,KAAI,GAAIL,WAAW,CAACG,KAAK,CAACG,WAAW,CAAC;MAC5C,OAAOL,gBAAgB,CAACG,iBAAiB,CAACG,MAAM,CAACC,UAAS,IACxDA,UAAU,CAACX,IAAI,CAACS,WAAW,CAAC,CAAC,CAACG,QAA